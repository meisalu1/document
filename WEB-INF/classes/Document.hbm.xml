<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="documents.model.data.Document" table="document">
      <id name="id" type="java.lang.Integer" column="document">
        <generator class="increment" />
      </id>
  <property name="name">
     <column name="name" />
  </property>
  <property name="desc">
    <column name="description"/>
  </property>
 </class>
</hibernate-mapping>

